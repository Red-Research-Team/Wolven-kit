<catel:UserControl
    x:Class="WolvenKit.Views.Editor.AssetBrowserView"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:catel="http://schemas.catelproject.com"
    xmlns:converters1="clr-namespace:WolvenKit.Functionality.Converters"
    xmlns:editor="clr-namespace:WolvenKit.ViewModels.Editor;assembly=WolvenKit.App"
    xmlns:hc="https://handyorg.github.io/handycontrol"
    xmlns:iconPacks="http://metro.mahapps.com/winfx/xaml/iconpacks"
    xmlns:svgc="http://sharpvectors.codeplex.com/svgc/"
    xmlns:syncfusion="http://schemas.syncfusion.com/wpf"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" mc:Ignorable="d"
    BorderBrush="{x:Null}"
    d:DataContext="{d:DesignInstance Type=editor:AssetBrowserViewModel}"
    >

    <catel:UserControl.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <hc:ThemeResources />
                <hc:Theme />
            </ResourceDictionary.MergedDictionaries>
        </ResourceDictionary>
    </catel:UserControl.Resources>
    <Grid>
        <Border
            hc:ThemeManager.RequestedAccentColor="{DynamicResource MahApps.Brushes.Accent3}"
            BorderBrush="{DynamicResource MahApps.Brushes.Accent4}"
            BorderThickness="0">
            <Grid>
                <Grid>
                    <Grid.ColumnDefinitions>
                        <ColumnDefinition Width="1*" />
                        <ColumnDefinition Width="3*" />
                    </Grid.ColumnDefinitions>

                    <Grid Grid.Column="0">
                        <Grid Grid.Column="0">
                            <Grid.Resources>
                                <converters1:ExtensionToImageConverter x:Key="ExtensionToImageConverter" />
                            </Grid.Resources>



                            <syncfusion:SfTreeNavigator
                                x:Name="TreeNavigator"
                                Header="Depot"
                                NavigationMode="Default"
                                ItemsSource="{Binding BoundRootNodes, Mode=OneWay}"
                                SelectedItem="{Binding SelectedNode, Mode=TwoWay, UpdateSourceTrigger=PropertyChanged}"
                                SelectionChanged="SfTreeNavigator_SelectionChanged">


                                <syncfusion:SfTreeNavigator.ItemTemplate>

                                    <HierarchicalDataTemplate ItemsSource="{Binding SubDirectories}">

                                        <StackPanel
                                            Margin="0,0,0,0"
                                            Background="Transparent"
                                            Orientation="Horizontal">
                                            <svgc:SvgViewbox
                                                Width="17"
                                                Height="17"
                                                Margin="-5,0,5,0"
                                                VerticalAlignment="Center"
                                                Source="{Binding Extension, Converter={StaticResource ExtensionToImageConverter}}"
                                                Stretch="Uniform" />
                                            <TextBlock
                                                VerticalAlignment="Center"
                                                Background="Transparent"
                                                FontSize="11"
                                                Foreground="White"
                                                Text="{Binding Name}" />
                                        </StackPanel>
                                    </HierarchicalDataTemplate>
                                </syncfusion:SfTreeNavigator.ItemTemplate>
                            </syncfusion:SfTreeNavigator>

                        </Grid>

                    </Grid>



                    <Grid Grid.Column="1">



                        <Grid
                            Height="38"
                            HorizontalAlignment="Stretch"
                            VerticalAlignment="Top">

                            <Grid.ColumnDefinitions>
                                <ColumnDefinition Width="*"/>
                                <ColumnDefinition Width="*"/>

                            </Grid.ColumnDefinitions>
                            <Border
                                Height="36"
                                VerticalAlignment="Top"
                                BorderBrush="{DynamicResource BackgroundBrush}"
                                BorderThickness="0">
                                <Border BorderBrush="{DynamicResource BorderAlt}" BorderThickness="0,1,0,0">
                                    <Grid>

                                        <Grid Background="{DynamicResource ContentBackgroundAlt2}" >
                                            <hc:SearchBar
                                                x:Name="SBBar"
                                                Margin="0,0,30,0"
                                                Padding="4,0,-25,0"
                                                VerticalAlignment="Center"
                                                Background="{DynamicResource Fluent.Ribbon.Brushes.RibbonTabItem.Active.Background}"
                                                BorderBrush="Transparent"
                                                FlowDirection="LeftToRight"
                                                MouseDown="SearchBar_MouseDown"
                                                Text="search"
                                                PreviewTextInput="SBBar_PreviewTextInput"
                                                TextChanged="SBBar_TextChanged"
                                                >

                                                <hc:Interaction.Triggers>
                                                    <hc:EventTrigger EventName="SearchStarted">
                                                        <hc:EventToCommand Command="{Binding SearchStartedCommand}" PassEventArgsToCommand="True" />
                                                    </hc:EventTrigger>
                                                </hc:Interaction.Triggers>
                                            </hc:SearchBar>
                                            <iconPacks:PackIconMaterial
                                                Margin="5,0,35,0"
                                                VerticalAlignment="Center"
                                                HorizontalAlignment="Right"
                                                BorderThickness="0"
                                                Kind="LayersSearchOutline" />
                                        </Grid>
                                        <Button
                                            Width="30"
                                            Height="36"
                                            Padding="8,-2,5,-5"
                                            HorizontalAlignment="Right"
                                            VerticalAlignment="Top"
                                            hc:IconElement.Geometry="{DynamicResource AssetBrowserHome}"
                                            Background="Transparent"
                                            BorderBrush="{DynamicResource BorderAlt}"
                                            BorderThickness="0,0,1,0"
                                            Command="{Binding HomeCommand}"
                                            RenderTransformOrigin="0.5,0.5"
                                            Style="{StaticResource ButtonIcon}"
                                            Visibility="Visible">
                                            <Button.RenderTransform>
                                                <TransformGroup>
                                                    <ScaleTransform />
                                                    <SkewTransform />
                                                    <RotateTransform Angle="180" />
                                                    <TranslateTransform />
                                                </TransformGroup>
                                            </Button.RenderTransform>
                                        </Button>

                                    </Grid>
                                </Border>
                            </Border>

                        </Grid>

                        <Grid Margin="0,51,0,0">
                            <Grid.RowDefinitions>
                                <RowDefinition Height="*" />
                                <RowDefinition Height="Auto" />
                            </Grid.RowDefinitions>

                            <syncfusion:SfDataGrid
                                x:Name="InnerList"
                                Margin="1,-15,0,0"
                                AllowDeleting="False"
                                AllowDraggingColumns="False"
                                AllowDraggingRows="False"
                                AllowEditing="False"
                                AllowResizingColumns="False"
                                AllowSorting="False"
                                AllowFiltering="True"
                                AutoGenerateColumns="False"
                                Background="{DynamicResource RegionBrush}"
                                ColumnSizer="Star"
                                IsManipulationEnabled="False"
                                RowHeight="22"
                                SelectedItem="{Binding SelectedFile,  Mode=TwoWay, UpdateSourceTrigger=PropertyChanged}"
                                SelectedItems="{Binding SelectedFiles, Mode=TwoWay, UpdateSourceTrigger=PropertyChanged}"
                                SelectionChanged="InnerList_SelectionChanged"
                                SelectionMode="Extended"
                                EnableDataVirtualization="True"
                                ItemsSource="{Binding ElementName=dataPager,Path=PagedSource}"
                                >

                                <syncfusion:SfDataGrid.RecordContextMenu>
                                    <ContextMenu>
                                        <MenuItem Header="Add selected items to Project" Command="{Binding AddSelectedCommand}" />
                                        <MenuItem Header="Export selected - Simple" />
                                        <MenuItem Header="Export selected - Advanced" />

                                    </ContextMenu>
                                </syncfusion:SfDataGrid.RecordContextMenu>

                                <syncfusion:SfDataGrid.Resources>
                                    <Style TargetType="syncfusion:GridHeaderCellControl">
                                        <Setter Property="BorderThickness" Value="0,0,0,1" />
                                        <Setter Property="BorderBrush" Value="{DynamicResource SecondaryRegionBrush}" />
                                    </Style>
                                    <Style TargetType="syncfusion:GridCell">
                                        <Setter Property="BorderThickness" Value="0,0,0,0" />
                                    </Style>

                                    <converters1:ExtensionToBitmapConverter x:Key="ExtensionToBmp" />
                                </syncfusion:SfDataGrid.Resources>

