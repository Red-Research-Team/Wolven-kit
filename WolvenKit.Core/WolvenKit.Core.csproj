<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>net5.0</TargetFramework>
      <TreatWarningsAsErrors>true</TreatWarningsAsErrors>
      <PackageId>WolvenKit.Core</PackageId>
      <Authors>Hambalk√≥ Bence, Moritz Baron</Authors>
      <Company>WolvenKit</Company>
      <Product>WolvenKit.Core</Product>
      <Description>File formats (The Witcher 3) for the WolvenKit Mod Editor.</Description>
      <PackageLicenseExpression>MIT</PackageLicenseExpression>
      <PackageProjectUrl>https://github.com/WolvenKit/Wolven-kit</PackageProjectUrl>
      <RepositoryUrl>https://github.com/WolvenKit/Wolven-kit</RepositoryUrl>
      <RepositoryType>git</RepositoryType>
      <PackageTags>wolvenkit, witcher3</PackageTags>
      <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
      <Version>1.0.0</Version>
  </PropertyGroup>

    <ItemGroup>
        <PackageReference Include="Catel.Core" Version="5.12.13" />
        <PackageReference Include="FastMember" Version="1.5.0" />
        <PackageReference Include="DotNetHelper.FastMember.Extension" Version="2.0.0" />
        <PackageReference Include="Microsoft.CodeAnalysis.CSharp" Version="3.9.0" />
        <PackageReference Include="CsvHelper" Version="27.0.2" />
        <PackageReference Include="protobuf-net.Core" Version="3.0.101" />

    </ItemGroup>

</Project>
